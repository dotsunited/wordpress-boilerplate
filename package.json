{
  "private": true,
  "name": "@dotsunited/wordpress-boilerplate",
  "version": "12.0.1",
  "author": "Dots United GmbH",
  "license": "MIT",
  "repository": "https://github.com/dotsunited/wordpress-boilerplate.git",
  "scripts": {
    "build": "webpack --progress",
    "watch": "webpack --progress --watch",
    "build-gutenberg": "webpack --config webpack-blocks.config.js --progress",
    "watch-gutenberg": "webpack --config webpack-blocks.config.js --progress --mode=development --watch",
    "tailwind:base": "tailwind build node_modules/tailwindcss/base.css -c assets/tailwind/src/config.js -o assets/tailwind/base.css",
    "tailwind:components": "tailwind build node_modules/tailwindcss/components.css -c assets/tailwind/src/config.js -o assets/tailwind/components.css",
    "tailwind:utilities": "tailwind build node_modules/tailwindcss/utilities.css -c assets/tailwind/src/config.js -o assets/tailwind/utilities.css",
    "tailwind:variables": "tailwind build assets/tailwind/src/variables.css -c assets/tailwind/src/config.js -o assets/tailwind/variables.css",
    "tailwind": "npm run tailwind:base && npm run tailwind:components && npm run tailwind:utilities && npm run tailwind:variables",
    "makepot-plugin": "cd public/wp-content/mu-plugins/wordpress-boilerplate/ && npx node-wp-i18n makepot --type plugin --domain-path languages",
    "makepot-theme": "cd public/wp-content/themes/wordpress-boilerplate/ && npx node-wp-i18n makepot --type theme --domain-path languages",
    "release": "release-it"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 10"
  ],
  "resolutions": {
    "postcss-focus-within": "^4.0.0"
  },
  "dependencies": {
    "classnames": "^1.2.2",
    "core-js": "^3.27.2",
    "ctrly": "^0.7.0",
    "domestique": "^1.7.0",
    "jquery": "^3.6.3",
    "lodash": "^4.17.21",
    "memize": "^1.1.0"
  },
  "devDependencies": {
    "@americanexpress/purgecss-loader": "^2.0.0",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "img-loader": "^3.0.2",
    "mini-css-extract-plugin": "1.6.2",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-focus-within": "^7.0.1",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "postcss-safe-parser": "^6.0.0",
    "release-it": "^15.6.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "~2.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
